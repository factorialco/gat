{
  "name": "@factorialco/gat",
  "version": "3.0.0",
  "description": "Write your GitHub Actions workflows using TypeScript",
  "bin": {
    "gat": "dist/cli.js"
  },
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "vitest run",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "author": "David Morcillo <david.morcillo@factorial.co>",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.14",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.184",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "commander": "^11.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/factorialco/gat.git"
  },
  "bugs": {
    "url": "https://github.com/factorialco/gat/issues"
  },
  "homepage": "https://github.com/factorialco/gat#readme"
}
